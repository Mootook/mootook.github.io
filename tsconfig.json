{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "allowJs": true,
    "types": [ "webpack-env" ],
    "baseUrl": "./",
    "paths": {
      "@/*": ["docs/.vuepress/src/*"],
      "@shared/*": ["docs/.vuepress/shared/*"],
      "@temp/*": ["docs/.vuepress/.temp/*"],
      
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "docs/.vuepress/src/**/*.ts",
    "docs/.vuepress/src/**/*.tsx",
    "docs/.vuepress/src/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx",
    "./docs/.vuepress/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
